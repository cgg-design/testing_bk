pipeline {
  agent any
  stages {
    stage('Smoke') {
      steps {
              bat '''
              pip install pytest
              cd Py_Fixtures
              python -m pytest -v -x test_smoke01.py
              cd ..\\
              '''
   
      }
    }
    stage('Build') {
      steps {
              bat '''
              cd Py_Fixtures
              python -m pytest -v test_module01.py
              cd ..\\
              '''
      }
    }
  }
}
